                    <script>
                    function validate() {
						var isValid = true;
						
                        
                            if ($("#tiebreak").val()=="") {
                                //alert("You must enter a tiebreak value");
								$('#modalPickCountLabel').html('<i class="fa fa-warning fa-lg"></i>&nbsp;&nbsp; Tiebreak Value Required');
								$('#modalPickCount .modal-body').html('<p>Your pool requires you to enter a tiebreak value.</p><p>Please enter what you predict to be the combined points scored by both teams in the game indicated.</p>');
								$('#modalPickCount').modal('show');
                                isValid = false;
                            }
                        
							var maxPickCount = 5;
							//Too few teams picked
                            if($("input:radio:checked").length <5) {
                                $('#modalPickCountLabel').html('<i class="fa fa-warning fa-lg"></i>&nbsp;&nbsp; ' + maxPickCount + ' Picks Required');
								$('#modalPickCount .modal-body').html('<p>Your pool requires you to pick ' + maxPickCount + ' teams each week.</p><p>Please pick ' + maxPickCount + ' teams before submitting your pick sheet.</p>');
								$('#modalPickCount').modal('show');
								isValid = false;
                            }
							//Too many teams picked
							else if ($("input:radio:checked").length > 5)
							{
								$('#modalPickCountLabel').html('<i class="fa fa-warning fa-lg"></i>&nbsp;&nbsp; ' + maxPickCount + ' Picks Required');
								$('#modalPickCount .modal-body').html('<p>Your pool requires you to pick a maximum of ' + maxPickCount + ' teams each week.</p><p>To remove picks, click on the pick again so it is no longer highlighted.</p>');
								$('#modalPickCount').modal('show');
								isValid = false;
							}
						
                        
						if (isValid) {
							okToClose = true;
						}
                        return  isValid;
